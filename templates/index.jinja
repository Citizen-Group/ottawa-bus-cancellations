{% extends 'layout.jinja' %}
{% block content %}
extends layout

block content
    <!-- Navigation -->
    <nav class="navbar navbar-expand-lg bg-secondary text-uppercase fixed-top" id="mainNav">
      <div class="container">
        <a class="navbar-brand js-scroll-trigger" href="#page-top">Ottawa LRT Status</a>
        <button class="navbar-toggler navbar-toggler-right text-uppercase font-weight-bold bg-primary text-white rounded" type="button" data-toggle="collapse" data-target="#navbarResponsive" aria-controls="navbarResponsive" aria-expanded="false" aria-label="Toggle navigation">
          Menu
          <i class="fas fa-bars"></i>
        </button>
        <div class="collapse navbar-collapse" id="navbarResponsive">
          <ul class="navbar-nav ml-auto">

            <li class="nav-item mx-0 mx-lg-1">
              <a class="nav-link py-3 px-0 px-lg-3 rounded js-scroll-trigger"> <span id="hoursAgo">00</span>h since last incident</a>
            </li>
            <li class="nav-item mx-0 mx-lg-1">
              <a class="nav-link py-3 px-0 px-lg-3 rounded js-scroll-trigger" href="#Latest">Latest</a>
            </li>
            <li class="nav-item mx-0 mx-lg-1">
              <a class="nav-link py-3 px-0 px-lg-3 rounded js-scroll-trigger" href="#Report">Report</a>
            </li>
            <li class="nav-item mx-0 mx-lg-1">
              <a class="nav-link py-3 px-0 px-lg-3 rounded js-scroll-trigger" href="#Station">Stations</a>
            </li>
            <li class="nav-item mx-0 mx-lg-1">
              <a class="nav-link py-3 px-0 px-lg-3 rounded js-scroll-trigger" href="#News">News</a>
            </li>
            <li class="nav-item mx-0 mx-lg-1">
              <a class="nav-link py-3 px-0 px-lg-3 rounded js-scroll-trigger" href="#Statistics">Statistics</a>
            </li>

          </ul>
        </div>
      </div>
    </nav>
    <!-- Masthead -->
    if currentStatus.overallStatus == "success"
      <header class="masthead bg-primary text-white text-center">
        <div class="container d-flex align-items-center flex-column">

          <!-- Masthead Avatar Image -->
          <img class="masthead-avatar mb-5" src="img/hotmessexpress_success.png" alt="The Hot Mess Express">

          <!-- Masthead Heading -->
          <h1 class="masthead-heading text-uppercase mb-0">The LRT is running*</h1>
          small(class="masthead-subheading mb-0") We report fast. The Mayor reports furious.

    else if currentStatus.overallStatus == "danger"
      <header class="masthead bg-danger text-white text-center">
        <div class="container d-flex align-items-center flex-column">

          <!-- Masthead Avatar Image -->
          <img class="masthead-avatar mb-5" src="img/hotmessexpress_danger.png" alt="The Hot Mess Express">

          <!-- Masthead Heading -->
          <h1 class="masthead-heading text-uppercase mb-0">The LRT is a hot mess right now</h1>

    else if currentStatus.overallStatus == "warning"
      <header class="masthead bg-warning text-white text-center">
        <div class="container d-flex align-items-center flex-column">

          <!-- Masthead Avatar Image -->
          <img class="masthead-avatar mb-5" src="img/hotmessexpress_warning.png" alt="The Hot Mess Express">

          <!-- Masthead Heading -->
          <h1 class="masthead-heading text-uppercase mb-0">The LRT is struggling with life right now</h1>          

    else
      <header class="masthead bg-secondary text-white text-center">
        <div class="container d-flex align-items-center flex-column">
          <!-- Masthead Avatar Image -->
          <img class="masthead-avatar mb-5" src="img/hotmessexpress_info.png" alt="The Hot Mess Express">

          <!-- Masthead Heading -->
          <h1 class="masthead-heading text-uppercase mb-0">The LRT is running fine???</h1>
          small(class="masthead-subheading mb-0") We are having trouble figuring out whats going right now. <br> Check back shortly.

    <!-- Icon Divider -->
    <div class="divider-custom divider-light">
      <div class="divider-custom-line"></div>
      <div class="divider-custom-icon">
        <i class="fas fa-train"></i>
      </div>
      <div class="divider-custom-line"></div>
    </div>

    <!-- Masthead Subheading -->
    p.masthead-subheading.mb-0 Last incident was #{events[0].timestamp} ago. Reason: #{events[0].reason.short}. 

    <!-- Scroll to Events -->
    <a class="btn btn-outline-light btn-social mx-1 mt-3 js-scroll-trigger mt-5" href="#Latest">
      <i class="fa fa-chevron-down"></i>
    </a>

      </div>
    </header>

    <!-- Quick Status Section -->
    section.copyright.py-4.text-center.text-white
      .container
        p.small.text-uppercase.mb-3 Quick station status
        p.small!= currentStatus.quickStatusBlock
        button.small.btn.btn-link
          a.js-scroll-trigger(href="#Station") Details

    section(class="page-section" id="Latest")
      .container
        .row
          .col
            <!-- Active Events Section Heading -->
            h2.page-section-heading.text-center.text-uppercase.text-secondary.mb-5 Last 5 Reports
            <!-- Icon Divider -->
            <div class="divider-custom divider-dark">
              <div class="divider-custom-line"></div>
              <div class="divider-custom-icon">
                <i class="fas fa-train"></i>
              </div>
              <div class="divider-custom-line"></div>
            </div>
            p.text-uppercase.small This is work in progress  
        .row
          .col
            <div class="accordion text-center" id="events">                 
              each val, index1 in events
                .mb-0
                  <div class="card">
                    .card-header(id="heading"+index)
                      .mb-0
                        .row                    
                          .col-2
                            span(class="text-black btn-social mt-1") #{val.timestamp}
                          .col-10
                            .row(style="padding-left: 12px")
                              .col
                                span!= val.statusBlock
                            .row
                              button(class="btn btn-link" type="button" data-toggle="collapse" data-target="#collapse"+index1 aria-expanded="true" aria-controls="collapseOne" style="width:100%")
                                span(class="text-uppercase text-secondary") #{val.reason.short}

                  .collapse(id="collapse"+index1 aria-labelledby="heading"+index1 data-parent="#events")
                    .card-body             
                      div.text-center 
                        p.small Reported Location: #{val.location.text}
                        h3.text-uppercase.text-secondary #{val.reason.long}
                        p.text-uppercase.text-secondary #{val.subtext}
                        span.small Report an update:

                        <a class="btn btn-sm btn-light mx-1" href="https://www.reddit.com/message/compose/?to=JKOttawa&subject=Reporting%20LRT%20Status&message=Location:%0ATime:%0AReason:%0A">
                          <i class="fab fa-reddit-alien"></i> Reddit
                        </a>
    
                        <a class="btn btn-sm btn-light" href="mailto:control.ottawa@gmail.com?subject=Reporting LRT Status&body=Time:%0ALocation:%0AReason:%0A%0A">
                          <i class="far fa-envelope"></i> Email
                        </a>         
            
                </div>

            </div>

    <!-- Report Section -->
    <section class="page-section" id="Report">
      .container
        .row
          .col-lg-6
            <!-- Contact Section Heading -->
            <h2 class="page-section-heading text-center text-uppercase text-secondary mb-0">Report an incident</h2>

            <!-- Icon Divider -->
            <div class="divider-custom">
              <div class="divider-custom-line"></div>
              <div class="divider-custom-icon">
                <i class="fas fa-exclamation"></i>
              </div>
              <div class="divider-custom-line"></div>
            </div>

            <!-- Report-->
            <div class="row">
              <div class="col mb-3">
                <p class="text-center lead">Send us a status report</p>
              </div>
            </div>
            <div class="row mb-5">
              <div class="col-lg-3 ml-auto">
                <div class="text-center mt-2">
                  <a class="btn btn-outline-dark" href="https://www.reddit.com/message/compose/?to=JKOttawa&subject=Reporting%20LRT%20Status&message=Location:%0ATime:%0AReason:%0A">
                    <i class="fab fa-reddit-alien"></i> Reddit
                  </a>
                </div>
              </div>
              <div class="col-lg-3 mr-auto">
                <div class="text-center mt-2">
                  <a class="btn btn-outline-dark" href="mailto:control.ottawa@gmail.com?subject=Reporting LRT Status&body=Time:%0ALocation:%0AReason:%0A%0A">
                    <i class="far fa-envelope"></i> Email
                  </a>
                </div>
              </div>
            </div>
            
          .col-lg-5
            <!-- Email Alerts -->
            <h2 class="page-section-heading text-center text-uppercase text-secondary mb-0">Sign up for email alerts</h2>

            <!-- Icon Divider -->
            <div class="divider-custom">
              <div class="divider-custom-line"></div>
              <div class="divider-custom-icon">
                <i class="fas fa-envelope"></i>
              </div>
              <div class="divider-custom-line"></div>
            </div>

            <!-- Email -->
            <div class="row">
              <div class="col mb-3">
                <div class="text-center mt-2">
                  <div class="portfolio-item mx-auto" data-toggle="modal" data-target="#emailMode">
                    button(class="btn btn-outline-dark") Signup
                  </div>
                </div>
              </div>
            </div>   

    </section>
    
    <!-- Latest Section -->
    <section class="page-section bg-primary text-white mb-0" id="Station">
      <div class="container">

        <!-- About Section Heading -->
        <h2 class="page-section-heading text-center text-uppercase text-white">Latest Station Statuses</h2>

        <!-- Icon Divider -->
        <div class="divider-custom divider-light">
          <div class="divider-custom-line"></div>
          <div class="divider-custom-icon">
            <i class="fas fa-star"></i>
          </div>
          <div class="divider-custom-line"></div>
        </div>

        <!-- About Section Content -->
        <div class="row">
          .col-lg-6
            <!-- Current Status per station -->
            <!-- Line 1 -->
            .accordion(id="stations")
              p.page-section-heading.text-center.text-uppercase.text-white Line 1   
              each val, index in currentStatus.stopStatus.l1
                .mb-0.text-center
                  .form-group("titleBlock")
                    p.lead
              
                      if val.status == "danger"
                        button.btn.btn-danger(type="button" data-toggle="collapse" data-target="#stationDetails"+index) <i class="fas fa-fire"></i> #{index}
                      else if val.status == "warning"
                        button.btn.btn-warning(type="button" data-toggle="collapse" data-target="#stationDetails"+index) <i class="fas fa-exclamation"></i> #{index}
                      else if val.status == "info"
                        button.btn.btn-info(type="button" data-toggle="collapse" data-target="#stationDetails"+index) <i class="fas fa-exclamation-circle"></i> #{index}
                      else if val.status == "success"
                        button.btn.btn-success(type="button" data-toggle="collapse" data-target="#stationDetails"+index) #{index}

                      .collapse.mb-2(id="stationDetails"+index data-parent="#stations")
                          p Detailed station status dropdown is coming soon ‚ù§ 
                          p (Unlike the LRT)            

          <div class="col-lg-6">
            <!-- Line 2 -->
            p.page-section-heading.text-center.text-uppercase.text-white Line 2                    
            each val, index in currentStatus.stopStatus.l2
              .mb-0.text-center
                .form-group("titleBlock")
                  p.lead 
                    <span id="example" data-toggle="tooltip" data-placement="right" data-title="#{val.tooltipText}">
                    if val.status == "danger"
                      <button class="btn btn-danger"><i class="fas fa-fire"></i> #{index}</button>
                    else if val.status == "warning"
                      <button class="btn btn-warning"><i class="fas fa-exclamation"></i> #{index}</button>
                    else if val.status == "info"
                      <button class="btn btn-info"><i class="fas fa-exclamation-circle"></i> #{index}</button>
                    else if val.status == "success"
                      <button class="btn btn-success"> #{index}</button>
                    </span>
          </div>
        </div>
        <!-- -->
        <div class="text-center mt-4">
        </div>

      </div>
    </section>

    section.page-section(id="News")
      .container.mb-5
        .row
          .col-md-6
            .row.mb-5
              .col 
                h2.page-section-heading.text-center.text-uppercase.text-secondary.mb-5 News
                ul
                    li  OC shatters previous record of -26 days without failure, with 24h of solid runtime. Rioting breaks out in Ottawa.
                    li  RTG asleep on the job as train fails at 2am. OcTranspo to source methamphetamines from Lowertown
                    li  Bus days away from retirement slain on Ottawa streets
                    li  Mayor to start the first Ottawa battle royale with 2 million dollar bounty
                    li  Councilor tells civil transit commission to basically stfu
                    li  OC does not consider quickly hidden dead trains a issue
                    li  Train surprisingly hasn't caught fire. OC vows "change is coming"
                    li  Slight breezes and sunlight might be the cause of door jams says OC
            .row.mb-5
              .col
                h2.page-section-heading.text-center.text-uppercase.text-secondary.mb-5 Latest transit reviews
                p 
                  i.fas.fa-star
                  i.fas.fa-star
                  i.fas.fa-star
                  i.fas.fa-star
                  i.fas.fa-star
                p "I was just at the Lyon stop, train shows up, no standing room at all, announcement says next train not arriving, said fuck it and called an Uber after already paying for my train ticket"<br> - <i>Happy Customer Kenata88</i></p>
                p.small.text-muted
                  a(href="https://www.reddit.com/message/compose/?to=JKOttawa&subject=LRT") Submit a review


          .col-md-6
            h2.page-section-heading.text-center.text-uppercase.text-secondary.mb-5 Media Feed
            p.text-center
              <a href="https://twitter.com/intent/tweet?button_hashtag=ottLRTAlert&ref_src=twsrc%5Etfw" class="twitter-hashtag-button" data-show-count="false">#ottLRTAlert</a>
                <script async src="https://platform.twitter.com/widgets.js" charset="utf-8"></script> or 
              <a href="https://twitter.com/intent/tweet?button_hashtag=OccasionalTransport&ref_src=twsrc%5Etfw" class="twitter-hashtag-button" data-show-count="false">#OccasionalTransport</a>
                <script async src="https://platform.twitter.com/widgets.js" charset="utf-8"></script>
            p.text-center and mention 
              a.bold(href="https://twitter.com/OccTranspo?ref_src=twsrc%5Etfw%7Ctwcamp%5Eembeddedtimeline%7Ctwterm%5Eprofile%3Aocctranspo&ref_url=http%3A%2F%2Flocalhost%3A8080%2F") @OccTranspo 
              | to be featured
            
            <a class="twitter-timeline" data-height="100%" href="https://twitter.com/occtranspo?ref_src=twsrc%5Etfw"></a> 
            <script async src="https://platform.twitter.com/widgets.js" charset="utf-8"></script>

    <!-- Stats Section -->
    <section class="page-section portfolio" id="Statistics">
      <div class="container">

        <!-- Stats Section Heading -->
        <h2 class="page-section-heading text-center text-uppercase text-secondary mb-0">Statistics</h2>

        <!-- Icon Divider -->
        <div class="divider-custom">
          <div class="divider-custom-line"></div>
          <div class="divider-custom-icon">
            <i class="fas fa-train"></i>
          </div>
          <div class="divider-custom-line"></div>
        </div>

        <!-- Stats Grid Items -->
        <div class="page-section-heading">
          h3.text-center A work in progress 
            small.text-primary (just like the LRT)
        .row.mt-5
          <!-- Portfolio Item 1 -->
          <div class="col-md-6 col-lg-4">
            <div class="portfolio-item mx-auto" data-toggle="modal" data-target="#recordModal">
              <div class="portfolio-item-caption d-flex align-items-center justify-content-center h-100 w-100">
                <div class="portfolio-item-caption-content text-center text-white">
                    p Records
                </div>
              </div>
              p DAYS WITHOUT INCIDENT STREAK
              p Cur: 0 DAYS
              P Rec: -27
       
            </div>
          </div>

           <!-- Portfolio Item 2 -->
          <div class="col-md-6 col-lg-4">
            <div class="portfolio-item mx-auto" data-toggle="modal" data-target="#chartModal">
              <div class="portfolio-item-caption d-flex align-items-center justify-content-center h-100 w-100">
                <div class="portfolio-item-caption-content text-center text-white">
                    p Charts
                </div>
              </div>
              <canvas id="myChart" width="400" height="400"></canvas>
              script.
